{
    "require": {
        "phpunit/phpunit": "^9.5.10",
        "squizlabs/php_codesniffer": "3.*",
        "slim/slim": "^4.1",
        "slim/http": "*",
        "slim/psr7": "*",
        "robmorgan/phinx": "^0.11",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "symfony/var-dumper": "*"
    },
    "autoload": {
